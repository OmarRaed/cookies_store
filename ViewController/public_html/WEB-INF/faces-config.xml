<?xml version="1.0" encoding="windows-1252"?>
<faces-config version="2.1" xmlns="http://java.sun.com/xml/ns/javaee">
  <navigation-rule>
    <from-view-id>/home.jsf</from-view-id>
    <navigation-case>
      <from-outcome>Account</from-outcome>
      <to-view-id>/account.jsf</to-view-id>
    </navigation-case>
    <navigation-case>
      <from-outcome>Logout</from-outcome>
      <to-view-id>/signin.jsf</to-view-id>
    </navigation-case>
  </navigation-rule>
  <navigation-rule>
    <from-view-id>/signin.jsf</from-view-id>
    <navigation-case>
      <from-outcome>success</from-outcome>
      <to-view-id>/home.jsf</to-view-id>
    </navigation-case>
  </navigation-rule>
  <navigation-rule>
    <from-view-id>/signup.jsf</from-view-id>
    <navigation-case>
      <from-outcome>account</from-outcome>
      <to-view-id>/home.jsf</to-view-id>
    </navigation-case>
  </navigation-rule>


   <managed-bean>
    <managed-bean-name>backing_offers</managed-bean-name>
    <managed-bean-class>com.fym.jsf.view.backing_offers</managed-bean-class>
    <managed-bean-scope>request</managed-bean-scope>
  </managed-bean>
   <managed-bean>
    <managed-bean-name>backing_details</managed-bean-name>
    <managed-bean-class>com.fym.jsf.view.backing_details</managed-bean-class>
    <managed-bean-scope>request</managed-bean-scope>
  </managed-bean>
  <navigation-rule>
    <from-view-id>/offerDetails.jsf</from-view-id>
    <navigation-case>
      <from-outcome>go to cart</from-outcome>
      <to-view-id>/cart.jsf</to-view-id>
    </navigation-case>
  </navigation-rule>
  <navigation-rule>
    <from-view-id>/offers.jsf</from-view-id>
    <navigation-case>
      <from-outcome>go details</from-outcome>
      <to-view-id>/offerDetails.jsf</to-view-id>
    </navigation-case>
  </navigation-rule>
  <navigation-rule>
    <from-view-id>/about.jsf</from-view-id>
    <navigation-case>
      <from-outcome>Account</from-outcome>
      <to-view-id>/account.jsf</to-view-id>
    </navigation-case>
    <navigation-case>
      <from-outcome>Logout</from-outcome>
      <to-view-id>/signin.jsf</to-view-id>
    </navigation-case>
  </navigation-rule>
  <navigation-rule>
    <from-view-id>/suggestions.jsf</from-view-id>
    <navigation-case>
      <from-outcome>Account</from-outcome>
      <to-view-id>/account.jsf</to-view-id>
    </navigation-case>
    <navigation-case>
      <from-outcome>Logout</from-outcome>
      <to-view-id>/signin.jsf</to-view-id>
    </navigation-case>
  </navigation-rule>
  <navigation-rule>
    <from-view-id>/account.jsf</from-view-id>
    <navigation-case>
      <from-outcome>Logout</from-outcome>
      <to-view-id>/signin.jsf</to-view-id>
    </navigation-case>
  </navigation-rule>
</faces-config>